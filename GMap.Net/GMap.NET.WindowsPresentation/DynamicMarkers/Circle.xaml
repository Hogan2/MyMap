<UserControl x:Class="GMap.NET.WindowsPresentation.Circle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
        <Ellipse Width="32" Height="32" Stroke="DarkBlue" StrokeThickness="0.5" Fill="#FC6355"/>
        <Ellipse Width="22" Height="22" Fill="#F0F5F8" Stroke="BlueViolet" StrokeThickness="0.5"/>
        <TextBlock Name="wayPointIndex" Foreground="DarkBlue" FontSize="14" Text="88" FontWeight="Medium"
                   TextAlignment="Center" Margin="0,7,0,0"/>
    </Grid>
</UserControl>
